#!/usr/bin/expect -f

set timeout -1

set args [lrange $argv 0 end]

set fd [open "request_input"]
set requestLine [split [read $fd] "\n"]
close $fd

spawn telnet {*}$args
    expect ".\r"

foreach req $requestLine {
    send -- "$req\n\r"  
}
    expect eof